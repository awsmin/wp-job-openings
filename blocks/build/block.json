{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wp-job-openings/blocks",
  "version": "1.0.0",
  "title": "",
  "category": "",
  "icon": "",
  "description": "",
  "attributes": {
    "search": {
      "type": "boolean",
      "default": false
    },
    "placement": {
      "type": "string",
      "default": "top"
    },
    "search_placeholder": {
      "type": "string",
      "default": ""
    },
    "filter_options": {
      "type": "array",
      "default": []
    },
    "listType": {
      "type": "string",
      "default": "all"
    },
    "layout": {
      "type": "string",
      "default": "list"
    },
    "selected_terms_main": {
      "type": "array",
      "default": []
    },
    "selectedTerms": {
      "type": "object",
      "default": {}
    },
    "orderBy": {
      "type": "string",
      "default": "new"
    },
    "hide_expired_jobs": {
      "type": "boolean",
      "default": false
    },
    "jobsPerPage": {
      "type": "number",
      "default": 5
    },
    "pagination": {
      "type": "string",
      "default": "modern"
    },
    "sort": {
      "type": "boolean",
      "default": false
    }
  },
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "wp-job-openings",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}