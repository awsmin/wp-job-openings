{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/job-list-block",
	"version": "0.1.0",
	"title": "Job List Block",
	"category": "widgets",
	"icon": "list-view",
	"description": "Job Listing BLock.",
	"example": {},
	"attributes": {
		"layout" : {
			"type": "string",
			"default" : "list"
		},
		"listing_per_page":{
			"type" :"integer",
			"default" : 10
		},
		"listing_order":{
			"type" :"string",
			"default" : "ascending"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "job-list-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
