{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wp-job-openings/wjo-block",
	"version": "0.2.0",
	"title": "Wjo Block",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"attributes": {
		"filter_options": {
			"type": "array",
			"default": "job-location,job-type,job-category"
		},
		"layout" : {
			"type": "string",
			"default" : "list"
		},
		"listing_per_page": {
			"type": "number",
			"default": 10
		  },
		"pagination": {
			"type": "string",
			"default" : "modern"
		},
		"search" : {
			"type": "boolean",
			"default" : true
		},
		"search_placeholder": {
			"type": "string",
			"default": ""
		},
		"enable_job_filter" : {
			"type": "boolean",
			"default" : true
		}
	},
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "wjo-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
